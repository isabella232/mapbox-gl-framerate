{
  "name": "@mapbox/mapbox-gl-framerate",
  "version": "0.1.2",
  "description": "A frame rate control to evaluate map rendering performance",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "rollup -c && rm -rf _site && mkdir -p _site && cp index.html _site/index.html && cp -a dist _site/dist",
    "watch": "rollup -c -w",
    "serve": "st -nc -i index.html",
    "start": "run-p serve watch",
    "lint": "eslint --no-eslintrc -c .eslintrc index.js",
    "format": "prettier --write '*.js'",
    "precommit": "lint-staged",
    "test": "npm run lint"
  },
  "keywords": [
    "fps",
    "mapbox"
  ],
  "author": "Mapbox",
  "license": "Copyright Â© Mapbox",
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint",
        "prettier --write",
        "git add"
      ]
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@mapbox/eslint-config-mapbox": "^1.2.1",
    "babel-eslint": "^10.0.3",
    "babel-preset-es2015-rollup": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^8.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-watch": "^4.3.1",
    "st": "^1.2.2"
  },
  "dependencies": {}
}
